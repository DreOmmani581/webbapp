{
    "ASEBDockerrunVersion" : 2,
    "containerDefinitions": [
        {
            "name": "webapp",
            "image": "dreommani/webapp",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 8080,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 443,
                    "hostPort": 6565,
                    "protocol": "tcp"
                }
            ],
            "memory": 500
        }
    ]
}