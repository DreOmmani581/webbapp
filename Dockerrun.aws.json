{
    "ASEBDockerrunVersion" : 2,
    "containerDefinitions": [
        {
            "name": "traefic",
            "image": "dreommani/trafik",
            "essential": false,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "memory": 300,
            "links": ["pgadmin","flower","backend","frontend"]
        },
        {
            "name": "db",
            "image": "dreommani/postgres",
            "essential": false,
            "memory": 200,
            "links": ["pgadmin","flower","celeryworker","backend"]
        },
        {
            "name": "pgadmin",
            "image": "dreommani/pgadmin",
            "essential": false,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 6565,
                    "protocol": "tcp"
                }
            ],
            "memory": 200
        },
        {
            "name": "rabbit",
            "image": "dreommani/rabbitmq:3",
            "essential": false,
            "memory": 100
        },
        {
            "name": "flower",
            "image": "dreommani/mher/flower:0.9.7",
            "essential": false,
            "memory": 100
        },
        {
            "name": "celeryworker",
            "image": "dreommani/celeryworker",
            "essential": false,
            "memory": 100
        },
        {
            "name": "backend",
            "image": "dreommani/backend",
            "essential": false,
            "memory": 500
        },
        {
            "name": "frontend",
            "image": "dreommani/frontend",
            "essential": false,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "memory": 300
        }
        
    ]
}